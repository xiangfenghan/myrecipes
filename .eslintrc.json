{
    "parser": "babel-eslint",
    "extends": [
        "prettier",
        "airbnb"
    ],
    "env": {
        "es6": true,
        "browser": true
    },
    "globals": {
        "API_URL": true
    },
    "rules": {
        "react/jsx-filename-extension": 0,
        "indent": ["error", 4, { "SwitchCase": 1 }]
    }
}